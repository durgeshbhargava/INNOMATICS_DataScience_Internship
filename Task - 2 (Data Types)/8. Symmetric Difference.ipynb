    "# Enter your code here. Read input from STDIN. Print output to STDOUT\n",
    "a,b = (int(input()),input().split())\n",
    "c,d = (int(input()),input().split())\n",
    "x = set(b)\n",
    "y = set(d)\n",
    "p = y.difference(x)\n",
    "q = x.difference(y)\n",
    "r = p.union(q)\n",
    "print('\\n'.join(sorted(r,key=int)))"
